---
import AdminLayout from '../../../layouts/AdminLayout.astro'
import SigninComponent from '../../../components/admin/Signin.tsx'
---

<script >
    import {isLogged, url} from '../../../utils.ts'
    const token = sessionStorage.getItem('token')
    if (isLogged(token)){
        window.location = `${url}/admin/edit`
    }
</script>

<AdminLayout>
    <SigninComponent client:load/>
</AdminLayout>
