---
import Layout from '../../../layouts/Layout.astro'
import SigninComponent from '../../../components/admin/Signin.tsx'
---

<script >
    import {isLogged} from '../../../utils.ts'
    const token = sessionStorage.getItem('token')
    if (isLogged(token)){
        window.location = "http://localhost:4321/admin/edit"
    }
</script>

<Layout>
    <SigninComponent client:load/>
</Layout>
