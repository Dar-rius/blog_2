---
import AdminLayout from '../../../layouts/AdminLayout.astro'
import SignupComponent from '../../../components/admin/Signup.tsx'
---

<script >
    import {isLogged, url} from '../../../utils.ts'
    const token = sessionStorage.getItem('token')
    if (!isLogged(token)){
        window.location = `${url}/admin/auth/signin`
    }
</script>

<AdminLayout>
    <SignupComponent client:load />
</AdminLayout>
