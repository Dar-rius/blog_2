---
import Layout from '../../../layouts/Layout.astro'
import SignupComponent from '../../../components/admin/Signup.tsx'
import {isLogged} from '../../../utils.ts'

let session : any
if (typeof window !== "undefined") {
    session = sessionStorage.getItem("token")
}
if (!isLogged(session)){
    console.log("good")
    return Astro.redirect('/admin/auth/signin')
} else {
    console.error("bad")
}
---

<Layout>
    <SignupComponent client:idle />
</Layout>
